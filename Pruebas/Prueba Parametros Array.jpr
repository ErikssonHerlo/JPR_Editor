
func ParamArray(int[] prueba){
	print(prueba[0]);
	print(prueba[1]);
	prueba[2] = 5
	print(prueba[2]);
    
}



main(){
	int[] numero = new int[3]

	numero[0] = 1
	numero[1] = 2
	numero[2] = 3 
	print("\n Funcion Parametros");   
ParamArray(numero);
	print("\n Llenado de Numero");
	print(numero[0]);
	print(numero[1]);
	print(numero[2]);

	

    
}













